<h1>Roster</h1>

<% @characters.each do |character| %>
    <%= link_to member_character_path(character), class: "character-card" do %>
        <div class="character-card-level"><%= character.level %></div>
        <div class="character-card-name"><%= character.name %></div>
        <div><%= character.playable_class %></div>
        <div><%= character.spec %></div>
    <% end %>
<% end %>